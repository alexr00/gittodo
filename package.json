{
    "name": "githubtodo",
    "displayName": "githubtodo",
    "description": "Creates quick links out of github issues",
    "version": "0.0.1",
    "publisher": "alexr00",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.findGitHubIssue"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.findGitHubIssue",
                "title": "Git Hub Todo"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.14"
    },
    "dependencies": {
        "@octokit/rest": "^15.2.6"
    },
    "repository": "https://github.com/alexr00/gittodo.git",
    "author": "Alex Ross",
    "license": "none",
    "private": true
}
